{
  "extends": [
    "react-app",
    "prettier",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "jsx-quotes": [
      1,
      "prefer-double"
    ]
  },
  "plugins": [
    "prettier"
  ],
  "overrides": [
    {
      "files": ["*.stories.@(ts|tsx|js|jsx|mjs|cjs)"],
      "rules": {
       "storybook/hierarchy-separator": "error",
       "storybook/default-exports": "off"
      }
    }
  ]
}